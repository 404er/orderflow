{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/binance/src",
  "monorepo": true,
  "root": "apps/binance",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/binance/tsconfig.app.json"
  },
  "projects": {
    "binance": {
      "type": "application",
      "root": "apps/binance",
      "entryFile": "main",
      "sourceRoot": "apps/binance/src",
      "compilerOptions": {
        "tsConfigPath": "apps/binance/tsconfig.app.json"
      }
    },
    "bybit": {
      "type": "application",
      "root": "apps/bybit",
      "entryFile": "main",
      "sourceRoot": "apps/bybit/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bybit/tsconfig.app.json"
      }
    },
    "gateio": {
      "type": "application",
      "root": "apps/gateio",
      "entryFile": "main",
      "sourceRoot": "apps/gateio/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateio/tsconfig.app.json"
      }
    },
    "okx": {
      "type": "application",
      "root": "apps/okx",
      "entryFile": "main",
      "sourceRoot": "apps/okx/src",
      "compilerOptions": {
        "tsConfigPath": "apps/okx/tsconfig.app.json"
      }
    },
    "bitget": {
      "type": "application",
      "root": "apps/bitget",
      "entryFile": "main",
      "sourceRoot": "apps/bitget/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bitget/tsconfig.app.json"
      }
