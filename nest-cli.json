{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/binance/src",
  "monorepo": true,
  "root": "apps/binance",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/binance/tsconfig.app.json"
  },
  "projects": {
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "binance": {
      "type": "application",
      "root": "apps/binance",
      "entryFile": "main",
      "sourceRoot": "apps/binance/src",
      "compilerOptions": {
        "tsConfigPath": "apps/binance/tsconfig.app.json"
      }
    },
    "bybit": {
      "type": "application",
      "root": "apps/bybit",
      "entryFile": "main",
      "sourceRoot": "apps/bybit/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bybit/tsconfig.app.json"
      }
    },
    "orderflow": {
      "type": "library",
      "root": "libs/@/orderflow",
      "entryFile": "index",
      "sourceRoot": "libs/@/orderflow/src",
      "compilerOptions": {
        "tsConfigPath": "libs/@/orderflow/tsconfig.lib.json"
      }
    },
    "binance-backfill": {
      "type": "application",
      "root": "apps/binance-backfill",
      "entryFile": "main",
      "sourceRoot": "apps/binance-backfill/src",
      "compilerOptions": {
        "tsConfigPath": "apps/binance-backfill/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "cache": {
      "type": "library",
      "root": "libs/cache",
      "entryFile": "index",
      "sourceRoot": "libs/cache/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cache/tsconfig.lib.json"
      }
    }
  }
}