      "compilerOptions": {
        "tsConfigPath": "apps/binance-backfill/tsconfig.app.json"
      }
    },
    "orderflow": {
      "type": "library",
      "root": "libs/orderflow",
      "entryFile": "index",
      "sourceRoot": "libs/orderflow/src",
      "compilerOptions": {
        "tsConfigPath": "libs/orderflow/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
