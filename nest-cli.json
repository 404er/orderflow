{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/orderflow-service/src",
  "monorepo": true,
  "root": "apps/orderflow-service",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/orderflow-service/tsconfig.app.json"
  },
  "projects": {
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "orderflow-service": {
      "type": "application",
      "root": "apps/orderflow-service",
      "entryFile": "main",
      "sourceRoot": "apps/orderflow-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orderflow-service/tsconfig.app.json"
      }
    },
    "orderflow-api": {
      "type": "application",
      "root": "apps/orderflow-api",
      "entryFile": "main",
      "sourceRoot": "apps/orderflow-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orderflow-api/tsconfig.app.json"
      }
    }
  }
}